{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "implicit-arrow-linebreak": "off",
    "comma-dangle": "off",
    "indent": "off",
    "no-trailing-spaces": "off",
    "react/destructuring-assignment": ["off", "always", { "ignoreClassFields": false }],
    "react/prop-types": "off",
    "import/prefer-default-export": "off",
    "camelcase": "off"
  }
}
